{
  "app": {
    "number_of_threads": 1,
    "document_root": "./",
    "upload_path": "./",
    "file_types": [],
    "locations": [],
    "max_connections": 100000,
    "load_dynamic_views": false,
    "log": {
      "use_spdlog": false,
      "log_path": "./",
