    "use_sendfile": false,
    "use_gzip": false,
    "use_brotli": false,
    "static_files_cache_time": -1,
    "idle_connection_timeout": 60,
    "client_max_body_size": "10M",
    "client_max_memory_body_size": "1M",
    "enable_server_header": false,
    "enable_date_header": true
  }
}